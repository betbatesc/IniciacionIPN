<div class="row">
  <div class="col m6 offset-m3 center">
    <h5>Ingresar</h5>

    <%= link_to "Ingresar con Google+", user_omniauth_authorize_path(:google_oauth2), class: "btn red darken-1" %>
    <%= link_to "Ingresar con Facebook", user_omniauth_authorize_path(:facebook), class: "btn blue darken-4" %>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <%= render partial: "shared/devisemes" %>

  <div class="input-field">    
    <%= f.email_field :email, :class => "validate", :placeholder => "Email"  %>
  </div>
  <div class="input-field">    
    <%= f.password_field :password, autocomplete: "off", :class => "validate", :placeholder => "Password" %>
  </div>
  <button class="btn waves-effect waves-light" type="submit" name="action">Ingresar</button>
<% end %>
  </div>
</div>